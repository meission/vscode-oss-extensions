{
	"name": "vscode-drawio",
	"displayName": "vscode-drawio",
	"description": "drawio editor",
	"publisher": "eightHundreds",
	"version": "1.0.3",
	"engines": {
		"vscode": "^1.44.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/eightHundreds/vscode-drawio"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:vscode-drawio.editor"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "vscode-drawio.editor",
				"displayName": "draw io editor",
				"selector": [
					{
						"filenamePattern": "*.drawio"
					},
					{
						"filenamePattern": "*.drawio.svg"
					}
				]
			}
		],
		"configuration": {
			"title": "vscode-drawio",
			"type": "object",
			"properties": {
				"vscode-drawio.debug": {
					"type": "boolean",
					"default": false,
					"description": "Specifies whether to show debug log"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"__metadata": {
		"id": "61d19076-1ebc-499f-96f8-dded083a9039",
		"publisherId": "4a4f257d-c879-44d1-a319-81985971a010",
		"publisherDisplayName": "eightHundreds"
	}
}